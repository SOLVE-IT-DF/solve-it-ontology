@prefix : <https://ontology.solveit-df.org/observable/> .
@prefix solve-it: <https://ontology.solveit-df.org/> .
@prefix solve-it-observable: <https://ontology.solveit-df.org/observable/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix uco-core: <https://ontology.unifiedcyberontology.org/uco/core/> .
@prefix uco-observable: <https://ontology.unifiedcyberontology.org/uco/observable/> .
@base <https://ontology.solveit-df.org/observable/> .

#################################################################
#    SOLVE-IT Observable Module
#    Observable objects and facets for digital forensics
#################################################################

<https://ontology.solveit-df.org/observable> rdf:type owl:Ontology ;
    owl:versionIRI <https://ontology.solveit-df.org/observable/1.0.0> ;
    owl:imports <https://ontology.unifiedcyberontology.org/uco/core/1.3.0> ,
                <https://ontology.unifiedcyberontology.org/uco/observable/1.3.0> ;
    dcterms:title "SOLVE-IT Observable Module"@en ;
    dcterms:description "Observable objects for SOLVE-IT digital forensics, including video frames and other forensic artifacts."@en ;
    dcterms:creator "SOLVE-IT Project" ;
    owl:versionInfo "0.0.1" ;
    rdfs:label "SOLVE-IT Observable Module"@en .

#################################################################
#    Classes
#################################################################

###  VideoFrame Observable Object
solve-it-observable:VideoFrame rdf:type owl:Class ;
    rdfs:subClassOf uco-observable:ObservableObject ;
    rdfs:label "Video Frame"@en ;
    rdfs:comment "A single frame extracted from a video file during forensic analysis."@en .

###  VideoFrame Facet (for properties)
solve-it-observable:VideoFrameFacet rdf:type owl:Class ;
    rdfs:subClassOf uco-core:Facet ;
    rdfs:label "Video Frame Facet"@en ;
    rdfs:comment "Properties specific to a video frame."@en .

#################################################################
#    Object Properties
#################################################################

solve-it-observable:hasVideoFrameFacet rdf:type owl:ObjectProperty ;
    rdfs:domain solve-it-observable:VideoFrame ;
    rdfs:range solve-it-observable:VideoFrameFacet ;
    rdfs:label "has video frame facet"@en ;
    rdfs:comment "Links a VideoFrame to its facet containing frame-specific properties."@en .

solve-it-observable:extractedFromVideo rdf:type owl:ObjectProperty ;
    rdfs:domain solve-it-observable:VideoFrame ;
    rdfs:range uco-observable:File ;
    rdfs:label "extracted from video"@en ;
    rdfs:comment "Links a VideoFrame to the source video file."@en .

#################################################################
#    Data Properties
#################################################################

solve-it-observable:frameNumber rdf:type owl:DatatypeProperty ;
    rdfs:domain solve-it-observable:VideoFrameFacet ;
    rdfs:range xsd:integer ;
    rdfs:label "frame number"@en ;
    rdfs:comment "The sequential number of this frame in the video."@en .

solve-it-observable:timestamp rdf:type owl:DatatypeProperty ;
    rdfs:domain solve-it-observable:VideoFrameFacet ;
    rdfs:range xsd:dateTime ;
    rdfs:label "timestamp"@en ;
    rdfs:comment "The timestamp when this frame appears in the video."@en .

solve-it-observable:width rdf:type owl:DatatypeProperty ;
    rdfs:domain solve-it-observable:VideoFrameFacet ;
    rdfs:range xsd:integer ;
    rdfs:label "width"@en ;
    rdfs:comment "Frame width in pixels."@en .

solve-it-observable:height rdf:type owl:DatatypeProperty ;
    rdfs:domain solve-it-observable:VideoFrameFacet ;
    rdfs:range xsd:integer ;
    rdfs:label "height"@en ;
    rdfs:comment "Frame height in pixels."@en .
