@prefix : <https://ontology.solveit-df.org/> .
@prefix solve-it: <https://ontology.solveit-df.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix uco-core: <https://ontology.unifiedcyberontology.org/uco/core/> .
@prefix uco-observable: <https://ontology.unifiedcyberontology.org/uco/observable/> .
@prefix uco-action: <https://ontology.unifiedcyberontology.org/uco/action/> .
@base <https://ontology.solveit-df.org/> .

#################################################################
#    SOLVE-IT Core Ontology
#    Main ontology file with imports and metadata
#################################################################

<https://ontology.solveit-df.org/> rdf:type owl:Ontology ;
    owl:versionIRI <https://ontology.solveit-df.org/1.0.0> ;
    owl:imports <https://ontology.unifiedcyberontology.org/uco/core/1.3.0> ,
                <https://ontology.unifiedcyberontology.org/uco/observable/1.3.0> ,
                <https://ontology.unifiedcyberontology.org/uco/action/1.3.0> ,
                <https://ontology.solveit-df.org/observable> ,
                <https://ontology.solveit-df.org/technique> ,
                <https://ontology.solveit-df.org/weakness> ,
                <https://ontology.solveit-df.org/mitigation> ;
    dcterms:title "SOLVE-IT Digital Forensics Ontology"@en ;
    dcterms:description "A modular ontology extending UCO/CASE for the SOLVE-IT digital forensics project. This core ontology imports modules for techniques, weaknesses, mitigations, and observables."@en ;
    dcterms:creator "SOLVE-IT Project" ;
    owl:versionInfo "0.0.1" ;
    rdfs:label "SOLVE-IT Ontology"@en ;
    rdfs:comment "Core ontology file - imports all SOLVE-IT modules. Use this file to import the complete SOLVE-IT ontology."@en .
